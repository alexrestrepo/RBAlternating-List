#tag ClassProtected Class AlternatingListInherits Listbox	#tag Event		Function CellBackgroundPaint(g As Graphics, row As Integer, column As Integer) As Boolean		  dim col as color		  		  col=ColorforCell(row,column)		  cellBackgroundPaint(g,row,column)		  if alternateRows then		    if row mod 2=0 and not me.selected(row) then		      if col=&c000000 then col=alternateColor		    elseif me.selected(row) then		      if col=&c000000 then col=highlightColor		    elseif col=&c000000 then		      col=&cffffff		    end if		    		    g.foreColor=col		    g.fillrect 0,0,g.width,g.height		    		    g.foreColor=&ceeeeee		    g.drawLine 0,g.height-1,g.width,g.height-1		    if displayColumnSeparator then g.drawLine g.width-1,0,g.width-1,g.height		  end if		  return true		End Function	#tag EndEvent	#tag Event		Function CellClick(row as Integer, column as Integer, x as Integer, y as Integer) As Boolean		  lastClickedRow=row		  lastclickedColumn=column		  		  return cellCLick(row,column,x,y)		End Function	#tag EndEvent	#tag Event		Function CellTextPaint(g As Graphics, row As Integer, column As Integer, x as Integer, y as Integer) As Boolean		  if me.selected(row) then		    g.bold=true		    g.foreColor=textColor		  else		    g.bold=cellBold(row,column)		  end if		  		  return cellTextPaint(g,row,column,x,y)		End Function	#tag EndEvent	#tag Event		Sub DoubleClick()		  if me.listindex>-1 then cellDoubleClick(lastClickedRow,lastClickedColumn)		  doubleClick		End Sub	#tag EndEvent	#tag Event		Sub Open()		  dim i as integer		  		  for i=0 to columnCount-1		    me.columnAlignmentOffset(i)=3		  next		  		  Open		End Sub	#tag EndEvent	#tag Hook, Flags = &h0		Event CellBackgroundPaint(g as graphics, row as integer, column as integer)	#tag EndHook	#tag Hook, Flags = &h0		Event CellClick(row as integer, column as integer, x as integer, y as integer) As boolean	#tag EndHook	#tag Hook, Flags = &h0		Event CellDoubleClick